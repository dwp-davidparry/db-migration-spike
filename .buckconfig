[alias]
  personal-details-docker-build = //src/uk/gov/dwp/personal/details/server:docker-build
  personal-details-client-docker = //src/uk/gov/dwp/example/personal/details/client:example-personal-details-client-docker

[download]
  in_build = True

[java]
  src_roots = src
  source_level = 8
  target_level = 8

[maven_repositories]
  central = https://repo1.maven.org/maven2

[project]
  ide = intellij
  ignore = build, .git